%section.new_fraud_type.container
  .row
    .span10.offset1
      = form_tag new_fraud_path, method: :get, class: 'form-horizontal' do
        .control-group
          = label_tag :fraud_type_id, "Fraud Type", class: "control-label"
          .controls
            = select_tag :fraud_type_id, fraud_type_options
        .controls
          = submit_tag "New Fraud Event", name: nil, class: "btn"